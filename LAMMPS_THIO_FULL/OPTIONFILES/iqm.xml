<options>

<iqm help="QM calculator for pairs" section="sec:iqm">
		<job_file>iqm.jobs</job_file>
                <map_file>system.xml</map_file>
                <openmp_threads>1</openmp_threads>
		<tasks>input,dft,parse,dftcoupling,gwbse,bsecoupling</tasks>
		<store></store>
        <gwbse_options>OPTIONFILES/gwbse_pair.xml</gwbse_options>
        <bsecoupling_options>OPTIONFILES/bsecoupling.xml</bsecoupling_options>
        <dftcoupling_options>
            <degeneracy>0.0</degeneracy>
            <levA>1</levA>
            <levB>1</levB>
        </dftcoupling_options>
        <dftpackage>OPTIONFILES/xtpdft_pair.xml</dftpackage>
        <readjobfile>
            <singlet>thiophene:s1</singlet>
            <triplet>thiophene:t1</triplet>
            <electron>thiophene:e1</electron>
            <hole>thiophene:h1</hole>
        </readjobfile>
</iqm>



</options>


